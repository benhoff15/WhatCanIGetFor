generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Adventure {
  id          String   @id @default(cuid())
  type        String
  title       String
  location    String
  price       Int
  description String
  date        String?
  duration    String?
  details     String
}
